            <style>
                .productCheckboxs,.productCheckboxsAll{
                    position:relative;
                }
                .productCheckboxs input,.productCheckboxsAll input{
                    position: absolute;
                    left:50%;
                    top:50%;
                    margin-left: -8.22px;
                    margin-top: -8.22px;
                }
            </style>
            
            <!-- ============================================================== -->
            <!-- Start Page Content here -->
            <!-- ============================================================== -->

            <div class="content-page">
                <div class="content">
                    <!-- Start Content-->
                    <div class="container-fluid">

                        <div class="row">
                            <div class="col-12">
                                <div class="card">
                                    <div class="card-body">
                                        <h4 class="mt-0 header-title">Yüklenen Ürünler</h4>
                                        <p class="text-muted font-14">
                                            Platform aracılığı ile çekilen ürünleriniz bu sayfadan görüntülenebilmektedir.
                                        </p>

                                        <div style="display:none;" id="clearSelectProducts"  st><button type="button" class="btn btn-danger mb-3"><i class="mdi mdi-trash-can"></i> Seçili Ürünleri Temizle</button></div>
                                        <form action="/products/deleteManyProduct" method="post" id="deleteManyForm"><input type="hidden" name="ids" value="" id="hiddenIds"></form>
                                        <div class="table-responsive">
                                            <table id="datatable-buttons" class="table table-striped table-bordered nowrap productsTable">
                                                <thead>
                                                    <tr>
                                                        <th class="productCheckboxsAll"><input class="form-check-input" type="checkbox"></th>
                                                        <th>Ürün Resmi</th>
                                                        <th>Ürün Adı</th>
                                                        <th>Ürün Grup ID</th>
                                                        <th>Stok Durumu</th>
                                                        <th>Ürün Grubunu Sil</th>
                                                        <th>Düzenle</th>
                                                    </tr>
                                                </thead>
                                                <tbody>
    
                                                    <% listProducts.forEach(function(item) { %>
                                                    <tr>
                                                        <td class="productCheckboxs"><input class="form-check-input" type="checkbox" value="<%= item.productGroupID %>"></td>
                                                        <td class="text-center">
                                                            <img src="https://cdn.dsmcdn.com/mnresize/128/192<%= item.productImages[0] %>" class="shadow" height="50px">
                                                            <% if(item.productImages[1] != undefined){ %>
                                                            <img src="https://cdn.dsmcdn.com/mnresize/128/192<%= item.productImages[1] %>" class="shadow" height="50px">
                                                            <% } %> 
                                                        </td>
                                                        <td><%= item.productName %></td>
                                                        <td><%= item.productGroupID %></td>
                                                        <td>
                                                            <% if(item.productLogControl == true){ %>
                                                                <button type="button" class="btn btn-warning rounded-pill waves-effect waves-light w-100 d-flex align-items-center">
                                                                    <span class="btn-label"><i class="mdi mdi-alert"></i></span>
                                                                    <span>Stok/Fiyat Güncellenmiş</span>
                                                                </button>
                                                            <% }else{ %>
                                                                <button type="button" class="btn btn-success rounded-pill waves-effect waves-light w-100 d-flex align-items-center">
                                                                    <span class="btn-label"><i class="mdi mdi-check-all"></i></span>
                                                                    <span class="text-center">Güncel Ürün</span>
                                                                </button>
                                                            <% } %>
                                                        </td>
                                                        <td><a class="btn btn-primary" href="/products/<%= item.productGroupID %>"><i class="fe-eye"></i> İncele/Düzenle</a></td>
                                                        <td><a class="btn btn-danger" onclick="return confirm('Ürün grubunu silmek istediğinizden emin misiniz?')" href="/products/deleteProduct/<%= item.productGroupID %>"><i class="fe-trash"></i> Sil</a></td>
                                                    </tr>
                                                    <% }) %>
    
    
                                                </tbody>
                                            </table>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <!-- end row -->

                        
                    </div> <!-- container-fluid -->

                </div> <!-- content -->


            </div>

            <!-- ============================================================== -->
            <!-- End Page content -->
            <!-- ============================================================== -->